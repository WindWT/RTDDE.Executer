                        <TextBox Grid.Column="0" Name="EnemyInfo_need_pt"></TextBox>
                        <TextBlock Grid.Column="1" Text="rev"/>
                        <TextBox Grid.Column="2" Name="EnemyInfo_rev_Enemy_g_id"></TextBox>
                        <TextBox Grid.Column="3" Name="EnemyInfo_rev_Enemy_id"></TextBox>
                        <TextBox Grid.Column="4" Name="EnemyInfo_rev_Enemy_name"></TextBox>
                    </Grid>-->
