<UserControl x:Class="RTDDE.Executer.Enemy"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:RTDDE="clr-namespace:RTDDE.Executer"
             mc:Ignorable="d" >
    <Grid Background="White" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Initialized="EnemyTab_Initialized">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="360"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <DataGrid Grid.Column="0" Grid.RowSpan="2" Name="EnemyDataGrid" AutoGenerateColumns="True" SelectionChanged="EnemyDataGrid_SelectionChanged" FrozenColumnCount="2" ></DataGrid>
        <StackPanel Grid.Column="1" Grid.Row="0">
            <Expander BorderThickness="0" Name="EnemySearchExpander" IsExpanded="False">
                <Expander.Header>
                    <Label Content="Search"></Label>
                </Expander.Header>
                <Expander.Content>
                    <StackPanel>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="50"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <Style TargetType="{x:Type TextBox}" BasedOn="{StaticResource {x:Type TextBox}}">
                                    <Setter Property="IsReadOnly" Value="False"></Setter>
                                    <EventSetter Event="TextChanged" Handler="EnemySearch_TextChanged"/>
                                </Style>
                            </Grid.Resources>
                            <TextBlock Grid.Row="0" Grid.Column="0" Text="id" />
                            <TextBox Grid.Row="0" Grid.Column="1" Name="EnemySearch_id"></TextBox>
                            <TextBlock Grid.Row="0" Grid.Column="2" Text="g_id"/>
                            <TextBox Grid.Row="0" Grid.Column="3" Name="EnemySearch_g_id"></TextBox>
                            <TextBlock Grid.Row="1" Grid.Column="0" Text="name"/>
                            <TextBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Name="EnemySearch_name"></TextBox>
                        </Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <Style TargetType="{x:Type ComboBox}" BasedOn="{StaticResource {x:Type ComboBox}}">
                                    <EventSetter Event="SelectionChanged" Handler="EnemySearch_SelectionChanged"/>
                                </Style>
                            </Grid.Resources>
                            <ComboBox Grid.Row="0" Grid.Column="0" Name="EnemySearch_category"></ComboBox>
                            <ComboBox Grid.Row="0" Grid.Column="1" Name="EnemySearch_style"></ComboBox>
                            <ComboBox Grid.Row="0" Grid.Column="2" Name="EnemySearch_kind"></ComboBox>
                            <ComboBox Grid.Row="0" Grid.Column="3" Name="EnemySearch_attribute"></ComboBox>
                            <ComboBox Grid.Row="0" Grid.Column="4" Name="EnemySearch_sub_a1"></ComboBox>
                            <Button Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="5" Name="EnemySearchClear" Click="EnemySearchClear_Click">Clear</Button>
                        </Grid>
                    </StackPanel>
                </Expander.Content>
            </Expander>
            <Separator/>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBox Grid.Column="0" Name="EnemyInfo_id"></TextBox>
                <TextBox Grid.Column="1" Name="EnemyInfo_g_id"></TextBox>
                <TextBox Grid.Column="2" Name="EnemyInfo_category"></TextBox>
                <TextBox Grid.Column="3" Name="EnemyInfo_name"></TextBox>
                <TextBox Grid.Column="4" Name="EnemyInfo_style"></TextBox>
            </Grid>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                    <ColumnDefinition Width="50"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Column="0" Text="kind"/>
                <TextBox Grid.Column="1" Name="EnemyInfo_kind"></TextBox>
                <TextBlock Grid.Column="2" Text="attribute"/>
                <TextBox Grid.Column="3" Name="EnemyInfo_attribute"></TextBox>
                <TextBox Grid.Column="4" Name="EnemyInfo_sub_a1"></TextBox>
            </Grid>
            <Grid>
                <Button Name="Enemy_ShowModelButton" Width="360" HorizontalAlignment="Left" Content="Show Model">
                    <Button.Triggers>
                        <EventTrigger RoutedEvent="Button.Click">
                            <BeginStoryboard>
                                <Storyboard Name="SB_ShowModel">
                                    <DoubleAnimation
                                    Storyboard.TargetName="Model" 
                                    Storyboard.TargetProperty="Width"
                                    To="360" Duration="0:0:0.300">
                                        <DoubleAnimation.EasingFunction>
                                            <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                                        </DoubleAnimation.EasingFunction>
                                    </DoubleAnimation>
                                    <DoubleAnimation
                                    Storyboard.TargetName="Enemy_HideModelButton" 
                                    Storyboard.TargetProperty="Width"
                                    To="360" Duration="0:0:0.300">
                                        <DoubleAnimation.EasingFunction>
                                            <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                                        </DoubleAnimation.EasingFunction>
                                    </DoubleAnimation>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Button.Triggers>
                </Button>
                <Button Name="Enemy_HideModelButton" Width="0" Panel.ZIndex="256" HorizontalAlignment="Right" Content="Hide Model">
                    <Button.Triggers>
                        <EventTrigger RoutedEvent="Button.Click">
                            <BeginStoryboard>
                                <Storyboard Name="SB_HideModel">
                                    <DoubleAnimation
                                    Storyboard.TargetName="Model" 
                                    Storyboard.TargetProperty="Width"
                                    To="0" Duration="0:0:0.300">
                                        <DoubleAnimation.EasingFunction>
                                            <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                                        </DoubleAnimation.EasingFunction>
                                    </DoubleAnimation>
                                    <DoubleAnimation
                                    Storyboard.TargetName="Enemy_HideModelButton" 
                                    Storyboard.TargetProperty="Width"
                                    To="0" Duration="0:0:0.300">
                                        <DoubleAnimation.EasingFunction>
                                            <QuadraticEase EasingMode="EaseInOut"></QuadraticEase>
                                        </DoubleAnimation.EasingFunction>
                                    </DoubleAnimation>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Button.Triggers>
                </Button>
            </Grid>
        </StackPanel>
        <ScrollViewer Grid.Column="1" Grid.Row="1" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Disabled">
            <StackPanel>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="50"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="50"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Grid.Column="0" Text="lv"/>
                    <TextBox Grid.Row="0" Grid.Column="1" Name="EnemyInfo_lv" IsReadOnly="False" LostFocus="EnemyInfo_lv_LostFocus"></TextBox>
                    <TextBlock Grid.Row="0" Grid.Column="2" Text="lv_max"/>
                    <TextBox Grid.Row="0" Grid.Column="3" Name="EnemyInfo_lv_max"></TextBox>
                    <TextBlock Grid.Row="0" Grid.Column="4" Text="DX_limit"/>
                    <TextBox Grid.Row="0" Grid.Column="5" Name="EnemyInfo_bonus_limit_base"></TextBox>
                    <TextBlock Grid.Row="1" Grid.Column="0" Text="HP"/>
                    <TextBox Grid.Row="1" Grid.Column="1" Name="EnemyInfo_HP" Width="250"></TextBox>
                    <TextBlock Grid.Row="2" Grid.Column="0" Text="ATK"/>
                    <TextBox Grid.Row="2" Grid.Column="1" Name="EnemyInfo_ATK" Width="250"></TextBox>
                    <TextBlock Grid.Row="3" Grid.Column="0" Text="HEAL"/>
                    <TextBox Grid.Row="3" Grid.Column="1" Name="EnemyInfo_HEAL" Width="250"></TextBox>
                    <TextBlock Grid.Row="1" Grid.Column="2" Text="pt"/>
                    <TextBox Grid.Row="1" Grid.Column="3" Name="EnemyInfo_pt"></TextBox>
                    <TextBlock Grid.Row="1" Grid.Column="4" Text="Sale"/>
                    <TextBox Grid.Row="1" Grid.Column="5" Name="EnemyInfo_sale"></TextBox>
                    <TextBlock Grid.Row="2" Grid.Column="2" Text="EXP"/>
                    <TextBox Grid.Row="2" Grid.Column="3" Name="EnemyInfo_EXP"></TextBox>
                    <TextBlock Grid.Row="2" Grid.Column="4" Text="~"/>
                    <TextBox Grid.Row="2" Grid.Column="5" Name="EnemyInfo_EXP_Next"></TextBox>
                    <TextBlock Grid.Row="3" Grid.Column="2" Text="lvupCost"/>
                    <TextBox Grid.Row="3" Grid.Column="3" Name="EnemyInfo_cost"></TextBox>
                    <TextBlock Grid.Row="3" Grid.Column="4" Text="mix_exp"/>
                    <TextBox Grid.Row="3" Grid.Column="5" Name="EnemyInfo_mix"></TextBox>
                </Grid>
                <StackPanel Name="EnemyRevStackPanel">
                    <!--<Grid Name="EnemyRevGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="50"></ColumnDefinition>
                            <ColumnDefinition Width="50"></ColumnDefinition>
                            <ColumnDefinition Width="50"></ColumnDefinition>
                            <ColumnDefinition Width="50"></ColumnDefinition>
